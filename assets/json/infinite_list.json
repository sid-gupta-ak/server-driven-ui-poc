{
  "type": "infinite_scrollable_list",
  "args": {
    "dataUrl": "https://something.example",
    "maxSize": 0,
    "itemTemplate": {
      "type": "list_tile",
      "args": {
        "title": {
          "type": "text",
          "args": {
            "text": "${pokemon['name']}"
          }
        },
        "subtitle": {
          "type": "text",
          "args": {
            "text": "${pokemon['url']}"
          }
        }
      }
    }
  }
}